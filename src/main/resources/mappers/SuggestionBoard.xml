<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.deathmatch.genius.mapper.SuggestBoard">

	<insert id ="insert">
		insert into tbl_suggestion_boar(userId,title,content,ref,step,depth,pno,secret)
		values(#{userId},#{title},#{content},#{ref},#{depth},#{step},#{pno},#{secret})
	</insert>
	
	
</mapper>
